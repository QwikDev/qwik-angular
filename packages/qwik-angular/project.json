{
  "name": "qwik-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/qwik-angular/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/qwik-angular",
        "main": "packages/qwik-angular/src/index.ts",
        "tsConfig": "packages/qwik-angular/tsconfig.lib.json",
        "assets": ["packages/qwik-angular/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs qwik-angular {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/qwik-angular/**/*.ts"]
      }
    }
  },
  "tags": []
}
